{
    "model": "lgbm",
    "config": {
        "features": [
            "Base_6",
            "BertPCA50",
            "Momentum",
            "Interaction",
            "SimultaneousPlatformCount",
            "PublisherPCA",
            "DeveloperPCA",
            "Series",
            "NumSeries",
            "NumSeriesNormalized",
            "SeriesLifespan"
        ],
        "cv": "stratified",
        "params": {
            "objective": "root_mean_squared_error",
            "metric": "root_mean_squared_error",
            "learning_rate": 0.01,
            "num_leaves": 50,
            "min_data_in_leaf": 100,
            "max_depth": 5,
            "subsample_freq": 1,
            "subsample": 0.7,
            "reg_alpha": 0.001,
            "reg_lambda": 0.001,
            "colsample_bytree": 0.3,
            "early_stopping_rounds": 100,
            "n_estimators": 10000,
            "seed": 42
        }
    }
}